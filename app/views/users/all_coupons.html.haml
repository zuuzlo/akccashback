-provide(:title, "All Kohls Coupons Home Page | #{@user.user_name}")
.page-header
  %h1 
    Homepage for #{@user.user_name}
= render 'shared/user_nav_tabs'
.tab-content
  .row
    .s5.l5
      %h4 All Kohls Coupons
      = render 'shared/coupon_tabs'
    .s7.l7  
      =render partial: 'shared/carousel', locals: { coupons: @cal_coupons } 
  .coupon_header
  .row    
    = render 'shared/coupon_tab_content'