-provide(:title, "All Kohl's Coupons | Search Results")
.row
  .s5.l5
    %h1 
      Search Results for 
      %i
        #{@term}
    = render 'shared/coupon_tabs'
  .s7.l7  
    - if display_ad? 
      =render partial: 'shared/ads_display'
    -else
      =render partial: 'shared/carousel', locals: { coupons: @cal_coupons }
.coupon_header
.row
  = render 'shared/coupon_tab_content'